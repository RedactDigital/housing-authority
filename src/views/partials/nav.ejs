<nav>
    <div class="nav-wrapper blue-grey">
        <a href="#!" class="brand-logo center">
            <%= title %>
        </a>
        <ul class="left hide-on-med-and-down">
            <% for( let i=0; i < navLinks.length; i++ ) { %>
                <li class="<% if(navLinks[i].active) { %>active<% } %>">
                    <a href="<%= navLinks[i].url %>">
                        <%= navLinks[i].name %>
                    </a>
                </li>
                <% } %>
        </ul>
    </div>
</nav>